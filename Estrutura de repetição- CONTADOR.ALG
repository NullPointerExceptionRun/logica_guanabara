Algoritmo "semnome"


Var
   valor,contador: inteiro


Inicio
   contador <- 0
   escreval("-------------------")
   escreval("CONTAR ATÉ QUANTO ? ")
   escreval("-------------------")
   leia(valor)
   enquanto (contador <= valor)faca
   escreva(contador)
      contador <- contador +1
   fimenquanto
   escreval("TERMINOU A CONTAGEM")

Fimalgoritmo