Algoritmo "AnaliseNomeLetrasUltimaLetra"

Var
    n:caractere

Inicio
      escreva("SEU NOME:")
      leia(n)
      escreval("TOTAL DE LETRAS:", compr(n))
      escreval("LETRAS MAIUSCULAS: ", maiusc(n))
      escreval("MINUSCULAS: ", minusc(n))
      escreval("PRIMEIRA LETRA", copia(n, 1, 2))
      escreval("ULTIMA LETRA", copia(n, 7, 1))

Fimalgoritmo