Algoritmo "Exercicio1_Procedimento"

// Autor(a)    : Nome do(a) aluno(a)
// Data atual  : 29/12/2021
Var

  x,y: inteiro

  procedimento conta(a,b: inteiro)
  Inicio
  escreval("O VALOR DE A FOI", a)
  escreval("O VALOR DE B FOI", b)
  escreval("SOMA TOTAL E RESOLUÇÃO :", a + b)


  fimprocedimento

inicio
  x <- 25
  y <- 55
  
  conta(x,y)


Fimalgoritmo