Algoritmo "MediaWhile"



var

   n, x, n1, media:inteiro


Inicio

   media <- 0
   n1 <- 0

   escreva("EXERCICIO WHILE:")
   leia(n)

   enquanto (n1 < n) faca
      escreva("EXERCICIO WHILE:")
      leia(x)

      media<- media + x
      n1 <- n1 + 1

   fimenquanto
   escreva("EXERCICIO COMPLETO:", media/n)



Fimalgoritmo