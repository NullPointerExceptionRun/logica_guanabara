Algoritmo "semnome"

Var

   n : inteiro

procedimento parouimpar(v : inteiro )
inicio
   se(v % 2 = 0) entao
      escreval("ESSE NUMERO É PAR")
   senao
      escreval("ESSE NUMERO É IMPAR")
   fimse

fimprocedimento
Inicio
   escreva("Digite um numero : ")
   leia(n)
   parouimpar(n)


Fimalgoritmo