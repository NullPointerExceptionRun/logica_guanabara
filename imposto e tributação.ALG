Algoritmo "semnome"


Var
     produto, imposto : inteiro
     resul: real

Inicio
       escreva("Qual o valor do produto? ")
       leia(produto)
       escreva("Digite o valor do imposto: ")
       leia(imposto)
       
       resul <- produto * imposto / 100
       
       escreva("O valor total de impostos é: ", resul:3:2)
       
       
       

Fimalgoritmo