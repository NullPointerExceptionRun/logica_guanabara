Algoritmo "semnome"

Var

   valor1:real

Inicio
   escreva("Digite o valor que voce tem: ")
   leia(valor1)
   se(valor1 >= 10000)entao
      escreva("Voce pode viajar para disney ")
   senao
      se(valor1 >= 5000) e (valor1 < 1000)entao
         escreva("Dinheiro para ver a familia")
      senao
         escreva("Voce não vai para lugar algum")
      fimse
   fimse




Fimalgoritmo